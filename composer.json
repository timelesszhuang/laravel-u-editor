{
  "name": "qiangbi/laravel-u-editor-with-xiumi",
  "description": "UEditor for laravel5. Support i18n. UEditor is a Rich Text Web Editor From Baidu.",
  "keywords": [
    "laravel",
    "ueditor",
    "laravel-u-editor",
    "laravel5 web text editor"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "qiangbi",
      "email": "support@qiangbi.net"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-fileinfo": "*",
    "illuminate/support": "5.*",
    "qiniu/php-sdk": "7.*"
  },
  "autoload": {
    "psr-4": {
      "qiangbi\\UEditor\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "qiangbi\\UEditor\\UEditorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
